# Docker Compose Environment Variables
# Copy this file to .env.docker and fill in your API keys

# Brave Search API
BRAVE_API_KEY=your_brave_api_key_here

# Perplexity API
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Gmail Configuration
GMAIL_ADDRESS=your-email@gmail.com
GMAIL_APP_PASSWORD=your_gmail_app_password_here

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Firecrawl API
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# WhatsApp Configuration (for whatsapp-mcp)
WHATSAPP_PHONE_NUMBER=your_phone_number_with_country_code

# Airbnb API (if applicable)
AIRBNB_API_KEY=your_airbnb_api_key_here
